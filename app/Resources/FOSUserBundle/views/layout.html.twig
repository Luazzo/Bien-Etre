{% extends "::base.html.twig"%}
{% block body %}
    <div class="main">
        <div class="main-inner">
            <div class="container">
                <div class="content">  
                    

                    {% for type, messages in app.session.flashBag.all %}
                        {% for message in messages %}
                            <div class="{{ type }}">
                                {{ message|trans({}, 'FOSUserBundle') }}
                            </div>
                        {% endfor %}
                    {% endfor %}

                    <div>
                        {% block fos_user_content %}
                            
                        {% endblock fos_user_content %}
                    </div>

                </div><!-- /.content -->
            </div><!-- /.container -->
        </div><!-- /.main-inner -->
    </div><!-- /.main -->
{% endblock%}
