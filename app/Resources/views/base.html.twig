            <script src="{{ asset('bundles/annuaireannuaire/libraries/colorbox/jquery.colorbox-min.js')}}" type="text/javascript"></script>

            <script src="{{ asset('bundles/annuaireannuaire/libraries/flot/jquery.flot.min.js')}}" type="text/javascript"></script>
            <script src="{{ asset('bundles/annuaireannuaire/libraries/flot/jquery.flot.spline.js')}}" type="text/javascript"></script>

            <script src="{{ asset('bundles/annuaireannuaire/libraries/bootstrap-select/bootstrap-select.min.js')}}" type="text/javascript"></script>

            <script src="http://maps.googleapis.com/maps/api/js?libraries=weather,geometry,visualization,places,drawing&amp;sensor=false" type="text/javascript"></script>

            <script type="text/javascript" src="{{ asset('bundles/annuaireannuaire/libraries/jquery-google-map/infobox.js')}}"></script>
            <script type="text/javascript" src="{{ asset('bundles/annuaireannuaire/libraries/jquery-google-map/markerclusterer.js')}}"></script>
            <script type="text/javascript" src="{{ asset('bundles/annuaireannuaire/libraries/jquery-google-map/jquery-google-map.js')}}"></script>

            <script type="text/javascript" src="{{ asset('bundles/annuaireannuaire/libraries/owl.carousel/owl.carousel.js')}}"></script>
            <script type="text/javascript" src="{{ asset('bundles/annuaireannuaire/libraries/bootstrap-fileinput/fileinput.min.js')}}"></script>

            <script src="{{ asset('bundles/annuaireannuaire/js/superlist.js')}}" type="text/javascript"></script>
            <script type="text/javascript" src="{{asset("bundles/annuaireannuaire/js/codepostal.js")}}"></script>
        
        {% endblock %}
